{
  "kind": "collectionType",
  "collectionName": "tools",
  "info": {
    "singularName": "tool",
    "pluralName": "tools",
    "displayName": "Tool"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "website": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "tools"
    },
    "tags": {
      "type": "json"
    },
    "createdat": {
      "type": "datetime"
    },
    "popularity": {
      "type": "decimal"
    },
    "stacks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::stack.stack",
      "mappedBy": "tools"
    },
    "vote": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::vote.vote",
      "mappedBy": "tool"
    },
    "illustration": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
